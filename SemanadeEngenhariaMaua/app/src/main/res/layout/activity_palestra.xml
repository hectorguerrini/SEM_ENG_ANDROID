<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_palestra"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="semanaeng.studio.com.semanadeengenhariamaua.activity.palestra">
    <include
        android:id="@+id/toolbarPrincipal"
        layout="@layout/toolbar"
        />
    <include
        android:id="@+id/imgbar"
        layout="@layout/imgbar"
        />
    <ImageView
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:srcCompat="@drawable/ic_arrow_back_white_24dp"
        android:id="@+id/imageButtonBack"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/button_back"
        android:layout_alignEnd="@+id/button_back" />

    <Button
        android:layout_width="40dp"
        android:id="@+id/button_back"
        android:background="@android:color/transparent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_height="30dp" />
</RelativeLayout>
