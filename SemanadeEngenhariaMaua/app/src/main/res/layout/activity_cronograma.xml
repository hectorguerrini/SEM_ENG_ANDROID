<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cronograma"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="semanaeng.studio.com.semanadeengenhariamaua.activity.cronograma">
    <include
        android:id="@+id/toolbarPrincipal"
        layout="@layout/toolbar"
        />
    <include
        android:id="@+id/imgbar"
        layout="@layout/imgbar"
        />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:srcCompat="@drawable/ic_arrow_back_white_24dp"
        android:id="@+id/imageButtonBack"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/button_back"
        android:layout_alignEnd="@+id/button_back" />

    <Button
        android:layout_width="50dp"
        android:id="@+id/button_back"
        android:background="@android:color/transparent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_height="50dp" />

    <ImageView
        android:layout_height="match_parent"
        app:srcCompat="@drawable/cronograma"
        android:id="@+id/image_cronograma"
        android:layout_below="@+id/toolbarPrincipal"
        android:layout_width="match_parent" />

</RelativeLayout>
